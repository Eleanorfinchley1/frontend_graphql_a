<section id="torch-toolbar">
  <div class="torch-container">
    <%= link "Edit", to: Routes.torch_covid_info_path(@conn, :edit), class: "torch-button" %>
  </div>
</section>

<section id="torch-header-and-content">
  <div class="torch-container">
    <div class="header">
      <h3>Covid info</h3>
    </div>
    <ul>

        <li>
          <div class="torch-show-label">Fields</div>
          <div class="torch-show-data"><pre><%= Jason.encode!(@covid_info.value, pretty: true) %></pre></div>
        </li>

    </ul>
  </div>
</section>