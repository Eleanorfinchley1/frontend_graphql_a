<section id="torch-toolbar">
  <div class="torch-container">
    <%= link "Cancel", to: Routes.torch_post_report_reason_path(@conn, :index), class: "torch-button" %>
  </div>
</section>

<section id="torch-header-and-content">
  <div class="torch-container">
    <div class="header">
      <h3>Edit Post report reason</h3>
    </div>
    <%= render "form.html", Map.put(assigns, :action, Routes.torch_post_report_reason_path(@conn, :update, @post_report_reason)) %>
  </div>
</section>
